<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    card_view:cardUseCompatPadding="false"
    card_view:cardElevation="10dp">

    <LinearLayout
        android:id="@+id/panelAddress"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#080C13"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/linearLayoutBar1"
            android:foreground="?android:attr/selectableItemBackground"
            android:gravity="center_horizontal">

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/viewBar1"
                android:background="@color/colorMajor" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:id="@+id/imageViewBar1"
                android:tint="@color/colorMajor"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="HOME"

                android:fontFamily="@font/fira_sans_semibold"
                android:textSize="9dp"
                android:id="@+id/textViewBar1"
                android:textColor="@color/colorMajor"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/linearLayoutBar2"
            android:foreground="?android:attr/selectableItemBackground"
            android:gravity="center_horizontal">

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/colorMajor"
                android:id="@+id/viewBar2"
                android:visibility="invisible"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:id="@+id/imageViewBar2"
                android:tint="@color/colorunSelected"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="COMMUNITY"
                android:fontFamily="@font/fira_sans_semibold"
                android:textSize="9dp"
                android:id="@+id/textViewBar2"
                android:tint="@color/colorunSelected"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/linearLayoutBar3"
            android:foreground="?android:attr/selectableItemBackground"
            android:gravity="center_horizontal">

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/colorMajor"
                android:id="@+id/viewBar3"
                android:visibility="invisible"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:id="@+id/imageViewBar3"
                android:tint="@color/colorunSelected"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="WORKOUT"
                android:textSize="9dp"
                android:fontFamily="@font/fira_sans_semibold"
                android:id="@+id/textViewBar3"
                android:tint="@color/colorunSelected"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/linearLayoutBar4"
            android:foreground="?android:attr/selectableItemBackground"
            android:gravity="center_horizontal">

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/colorMajor"
                android:id="@+id/viewBar4"
                android:visibility="invisible"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:id="@+id/imageViewBar4"
                android:tint="@color/colorunSelected"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="CHALLENGE"
                android:textSize="9dp"
                android:id="@+id/textViewBar4"
                android:fontFamily="@font/fira_sans_semibold"
                android:tint="@color/colorunSelected"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/linearLayoutBar5"
            android:foreground="?android:attr/selectableItemBackground"
            android:gravity="center_horizontal">

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/colorMajor"
                android:id="@+id/viewBar5"
                android:visibility="invisible"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:id="@+id/imageViewBar5"
                android:tint="@color/colorunSelected"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="PROFIL"
                android:textSize="9dp"
                android:id="@+id/textViewBar5"
                android:fontFamily="@font/fira_sans_semibold"
                android:tint="@color/colorunSelected"/>

        </LinearLayout>

    </LinearLayout>

</androidx.cardview.widget.CardView>